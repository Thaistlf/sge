    {% extends ':template:template.html.twig' %}



    {% block contentheader %}

    <section class="content-header">
        <h1>
            InformacaoComplementar
            <small>Mostrar</small>
        </h1>
    </section>

    {% endblock %}

    {% block firstrow %}

    {% for message in app.session.flashBag.get('notice') %}
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="alert alert-success alert-dismissible" role="alert">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <i class="fa fa-info"></i> {{ message }}. <i class="fa fa-smile-o"></i>
        </div>
    </div>
    {% endfor %}


    {% endblock %}


    {% block panelheading %}

        <div class="row">

    <div class="col-md-2 col-sm-3 col-xs-3">

        <a href="{{ path('cadastro_aluno_show', { 'id' : informacaoComplementar.aluno.id }) }}" class="btn btn-primary btn-sm link-search">
            <span class="fa fa-reply"></span>
            Mostrar Aluno
        </a>

    </div>
        <div class="col-md-2 col-sm-3 col-xs-3">

            <a href="{{ path('cadastro_informacaocomplementar_edit', { 'id': informacaoComplementar.id }) }}" class="btn btn-primary btn-sm link-search">
                <span class="glyphicon glyphicon-edit"></span>
                Editar
            </a>

        </div>
        <div class="col-md-2 col-sm-3 col-xs-3">

        </div>
</div>{% endblock %}



    {% block panelbody %}

    <div class="table table-responsive">
        <table class="table table-bordered table-condensed">
            <tbody>
                <tr>
                    <th>Professor</th>
                        <td>{{ informacaoComplementar.professor }}</td>
                </tr>
                <tr>
                    <th>Problemassaude</th>
                        <td>{{ informacaoComplementar.problemasSaude }}</td>
                </tr>
                <tr>
                    <th>Observacao</th>
                        <td>{{ informacaoComplementar.observacao }}</td>
                </tr>
                <tr>
                    <th>Id</th>
                        <td>{{ informacaoComplementar.id }}</td>
                </tr>
            </tbody>
        </table>
    </div>


    {% endblock %}
